<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:showIn="@layout/content_song_list_header">

    <ImageView
        android:id="@+id/iv_song_header_search"
        style="@style/StyleHeaderImage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/iv_song_header_sort"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@android:drawable/ic_menu_search" />

    <ImageView
        android:id="@+id/iv_song_header_sort"
        style="@style/StyleHeaderImage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/iv_song_header_edit"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/sort_white" />

    <ImageView
        android:id="@+id/iv_song_header_edit"
        style="@style/StyleHeaderImage"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/edit" />

</merge>